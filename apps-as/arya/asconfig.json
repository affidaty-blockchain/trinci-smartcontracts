{
    "targets": {
        "debug": {
            "binaryFile": "build/debug.wasm",
            "textFile": "build/debug.wat",
            "debug": true
        },
        "release": {
            "binaryFile": "build/release.wasm",
            "textFile": "build/release.wat",
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": false,
            "noAssert": false
        }
    },
    "options": {
        "sourceMap":true,
        "importMemory": false,
        "exportRuntime": false,
        "memoryBase": 0
    }
}